<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#F4F5F7">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:background="@android:color/white"
        android:layout_height="44dp">

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="44dp"
            android:gravity="center"
            android:text="AGV调度"
            android:textColor="#ff333333"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageView
            android:id="@+id/iv_history"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:src="@mipmap/ic_history"
            android:paddingEnd="17dp"
            android:visibility="gone"
            />


    </androidx.constraintlayout.widget.ConstraintLayout>



    <LinearLayout
        android:id="@+id/ll_cgrk"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="16dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="13dp"
        android:background="@drawable/shape_rectangular_white_3_bg">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="采购到货检验入库"
            android:layout_marginTop="17dp"
            android:layout_marginStart="13dp"
            android:textColor="#ff333333"
            android:textSize="15sp"
            />


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rc_cgrk"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="14dp"
            />


    </LinearLayout>


    <LinearLayout
        android:id="@+id/ll_wxrk"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="13dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="13dp"
        android:background="@drawable/shape_rectangular_white_3_bg">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="外协检验入库"
            android:layout_marginTop="17dp"
            android:layout_marginStart="13dp"
            android:textColor="#ff333333"
            android:textSize="15sp"
            />


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rc_wxrk"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="14dp"
            />


    </LinearLayout>


    <LinearLayout
        android:id="@+id/ll_scll"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="13dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="13dp"
        android:background="@drawable/shape_rectangular_white_3_bg">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="生产领料"
            android:layout_marginTop="17dp"
            android:layout_marginStart="13dp"
            android:textColor="#ff333333"
            android:textSize="15sp"
            />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rc_scll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="14dp"
            />


    </LinearLayout>




    <LinearLayout
        android:id="@+id/ll_wxfl"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="13dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="13dp"
        android:background="@drawable/shape_rectangular_white_3_bg">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="外协发料"
            android:layout_marginTop="17dp"
            android:layout_marginStart="13dp"
            android:textColor="#ff333333"
            android:textSize="15sp"
            />


        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rc_wxfl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="14dp"
            />




    </LinearLayout>





    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cons_wgjyrk"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="13dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="13dp"
        android:background="@drawable/shape_rectangular_white_3_bg">

        <TextView
            android:id="@+id/tv_rk"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="完工检验入库"
            android:layout_marginTop="17dp"
            android:layout_marginStart="13dp"
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            />



        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rc_wgrk"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="14dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_rk"
            />



    </androidx.constraintlayout.widget.ConstraintLayout>









    <LinearLayout
        android:id="@+id/ll_xsfh"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="13dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="13dp"
        android:background="@drawable/shape_rectangular_white_3_bg">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="销售发货"
            android:layout_marginTop="17dp"
            android:layout_marginStart="13dp"
            android:textColor="#ff333333"
            android:textSize="15sp"
            />



        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rc_xsfl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="14dp"
            />



    </LinearLayout>





    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="16dp"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="13dp"
        android:visibility="gone"
        android:layout_marginBottom="15dp"
        android:background="@drawable/shape_rectangular_white_3_bg">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="模具领用与归还"
            android:layout_marginTop="17dp"
            android:layout_marginStart="13dp"
            android:textColor="#ff333333"
            android:textSize="15sp"
            />


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="14dp">




            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableTop="@mipmap/agv_mj_cx"
                android:gravity="center"
                android:drawablePadding="7dp"
                android:text="模具至\n产线"
                android:textColor="#ff5e5d5d"
                android:textSize="12sp"
                />
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"/>
            <TextView
                android:id="@+id/tv_hjagv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableTop="@mipmap/agv_xc"
                android:gravity="center"
                android:drawablePadding="7dp"
                android:text="呼叫空AGV\n小车"
                android:textColor="#ff5e5d5d"
                android:textSize="12sp"
                />
            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"/>
            <TextView
                android:id="@+id/tv_cxmork"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableTop="@mipmap/agv_cxmjrk"
                android:gravity="center"
                android:drawablePadding="7dp"
                android:text="产线磨模\n具入库"
                android:textColor="#ff5e5d5d"
                android:textSize="12sp"
                />

            <View
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_weight="1"/>

            <TextView
                android:id="@+id/tv_khj_mjk"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableTop="@mipmap/agv_khj_mjk"
                android:gravity="center"
                android:text="空货架至\n模具库"
                android:drawablePadding="7dp"
                android:textColor="#ff5e5d5d"
                android:textSize="12sp"
                android:visibility="invisible"
                />



        </LinearLayout>


    </LinearLayout>



    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cons_jcyw"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="13dp"
        android:layout_marginEnd="13dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="13dp"
        android:background="@drawable/shape_rectangular_white_3_bg"
        >

        <TextView
            android:id="@+id/tv_jcyw"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="基础业务"
            android:layout_marginTop="17dp"
            android:layout_marginStart="13dp"
            android:textColor="#ff333333"
            android:textSize="15sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rc_jcyw"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="14dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@id/tv_jcyw"
            />



    </androidx.constraintlayout.widget.ConstraintLayout>




</LinearLayout>
</androidx.core.widget.NestedScrollView>