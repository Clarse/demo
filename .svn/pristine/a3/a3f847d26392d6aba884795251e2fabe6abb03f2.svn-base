<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="27dp"
    android:layout_marginEnd="27dp"
    android:orientation="vertical"
    android:background="@drawable/shape_rectangular_white_5_bg">

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="24dp"
        android:text="处理描述"
        android:textColor="#ff333333"
        android:textSize="16sp" />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp">


        <EditText
            android:id="@+id/et_ms"
            android:layout_width="match_parent"
            android:layout_height="160dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:paddingEnd="6dp"
            android:paddingStart="6dp"
            android:paddingTop="6dp"
            android:textColor="#666666"
            android:gravity="top"
            android:textSize="13sp"
            android:hint="请输入处理描述"
            android:background="@drawable/shape_rectangular_white_3_bg"
            />


    </LinearLayout>








    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="35dp"
        android:layout_marginBottom="19dp"
        android:gravity="center">

        <Button
            android:id="@+id/btn_cancle"
            android:layout_width="110dp"
            android:layout_height="34dp"
            android:text="取消"
            android:textColor="#ff333333"
            android:textSize="15sp"
            android:background="@drawable/shape_rectangular_agv_btn_bg"
            />


        <Button
            android:id="@+id/btn_ok"
            android:layout_width="110dp"
            android:layout_height="34dp"
            android:text="确定"
            android:layout_marginStart="24dp"
            android:background="@drawable/shape_rectangular_agv_btn_ok"
            android:textColor="#ffffffff"
            android:textSize="15sp"
            />



    </LinearLayout>





</LinearLayout>
